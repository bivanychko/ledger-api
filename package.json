{
  "name": "ledger-api",
  "version": "0.0.1",
  "description": "ledger-api",
  "author": "Bohdan Ivanychko",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format:check": "npx prettier \"src/**/*.ts\" --check",
    "format:fix": "npm run format:check -- --write",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "npm run typeorm migration:run",
    "migration:generate": "npm run typeorm migration:generate $npm_config_path/migrations/$npm_config_name",
    "seed:revert": "npm run typeorm:seed migration:revert",
    "seed:run": "npm run typeorm:seed migration:run",
    "seed:create": "typeorm migration:create $npm_config_path/seeds/$npm_config_name",
    "typeorm": "typeorm-ts-node-commonjs -d src/orm/migration.config.ts",
    "typeorm:seed": "typeorm-ts-node-commonjs -d src/orm/seed.config.ts"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/swagger": "^7.1.11",
    "@nestjs/typeorm": "^10.0.0",
    "@types/compression": "^1.7.3",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "helmet": "^7.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
